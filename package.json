{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start-dev": "node index.js",
    "start": "node dist/main.js",
    "build": "tsc",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/microservices": "^5.4.0",
    "@nestjs/testing": "^5.4.0",
    "@nestjs/websockets": "^5.4.0",
    "@types/node": "^10.7.1",
    "cache-manager": "^2.9.0",
    "cache-manager-redis-store": "^1.5.0",
    "http": "0.0.0",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "stream": "0.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
